<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">


    <changeSet id="006" author="ad">
        <createTable tableName="restaurant_info">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" />
            </column>
            <column name="name" type="VARCHAR(100)" />
            <column name="min_price" type="DECIMAL" />
            <column name="free_delivery" type="DECIMAL" />
            <column name="delivery" type="DECIMAL" />

        </createTable>

    </changeSet>
</databaseChangeLog>